#include <iostream>
#include <string>

#include "TestST.h"
#ifdef ORDERED
#include "TestOrderedST.h"
#endif
#ifdef STRING
#include "TestStringST.h"
#endif
#ifdef BALANCED_TREE
#include "TestBalancedTree.h"
#endif
#include "@ST@.h"

int main(int argc, char *argv[]) {
#ifndef STRING
    algs4::@ST@<std::string, int> st{@ST_INIT_ARGS@};
#else
    algs4::@ST@<int> st{@ST_INIT_ARGS@};
#endif
    TestST::init(st, std::cin, std::cout);
    std::cout << std::endl;
    TestST::testKeys(st, std::cout);
    std::cout << std::endl;
#ifdef ORDERED
    TestST::testOrderedST(st, std::cout);
    std::cout << std::endl;
#endif
#ifdef STRING
    TestST::testStringST(st, std::cout);
    std::cout << std::endl;
#endif
    TestST::removeSome(st, std::cout);
    std::cout << std::endl;
    TestST::removeAll(st, std::cout);
#ifdef BALANCED_TREE
    if (argc > 1) {
        std::cout << std::endl;
        algs4::@ST@<int, int> st2{@ST_INIT_ARGS@};
        TestST::testRedBlackBST(st2, std::stoi(argv[1]), std::cout);
    }
#endif
    return 0;
}

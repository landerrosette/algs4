#include <cassert>
#include <iostream>
#include <string>
#include <vector>

#include "algs4/@SORTER@.hpp"
#include "algs4/SortUtils.hpp"

int main(int argc, char *argv[]) {
    std::vector<std::string> a;
    for (std::string item; std::cin >> item;)
        a.push_back(item);
    algs4::@SORTER@::sort(a);
    assert(algs4::SortUtils::isSorted(a));
    algs4::SortUtils::show(a);
    return 0;
}

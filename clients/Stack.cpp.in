/******************************************************************************
 * % more tobe.txt
 * to be or not to - be - - that - - - is
 *
 * % ./@STACK@ < tobe.txt
 * to be not that or be (2 left on stack)
 ******************************************************************************/

#include "algs4/@STACK@.hpp"

#include <iostream>
#include <string>
#include <utility>

int main(int argc, char* argv[]) {
    algs4::@STACK@<std::string> s;
    for (std::string item; std::cin >> item;) {
        if (item != "-")
            s.push(std::move(item));
        else if (!s.isEmpty())
            std::cout << s.pop() << " ";
    }
    std::cout << "(" << s.size() << " left on stack)" << std::endl;
    return 0;
}
